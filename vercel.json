{
  "build": {
    "env": {
      "HUGO_VERSION": "0.129.0"
    }
  },
  "buildCommand": "hugo --minify --environment production",
  "outputDirectory": "./public",
  "headers": [
    {
      "source": "/(.*).json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json; charset=utf-8"
        }
      ]
    }
  ]
}